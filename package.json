{
    "name": "nextjs-ts-auto-graphql",
    "version": "1.0.0",
    "private": false,
    "dependencies": {
        "@apollo/client": "^3.4.11",
        "@material-ui/core": "^4.12.3",
        "@prisma/client": "^3.0.2",
        "@types/graphql-fields": "^1.3.4",
        "apollo-server-core": "^3.3.0",
        "apollo-server-micro": "^3.3.0",
        "class-validator": "^0.13.1",
        "graphql": "^15.5.3",
        "graphql-fields": "^2.0.3",
        "graphql-scalars": "^1.10.1",
        "micro": "^9.3.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.15.3",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1"
    },
    "keywords": [
        "Next.js",
        "GraphQL",
        "TypeScript",
        "Prisma",
        "Apollo",
        "ESLint",
        "Prettier",
        "React",
        "React Hook Form",
        "Jest",
        "Testing Library React",
        "Material-UI"
    ],
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "scripts": {
        "build": "next build",
        "dev": "prisma generate && concurrently \"next\" \"npm run watch-queries\"",
        "format": "prisma format &&  prettier --write . && eslint --fix .",
        "lint": "eslint .",
        "start": "next start",
        "test": "jest",
        "type-check": "tsc",
        "quality": "npm run type-check && npm run lint && npm run test",
        "generate": "prisma generate",
        "migrate": "prisma migrate dev",
        "deploy": "prisma migrate deploy",
        "reset": "prisma migrate reset",
        "seed": "prisma db seed",
        "studio": "prisma studio",
        "prisma": "prisma format && npm run migrate && npm run seed",
        "generate-types": "graphql-codegen --config codegen.yml",
        "watch-queries": "graphql-codegen --config codegen.yml --watch",
        "postinstall": "prisma generate"
    },
    "devDependencies": {
        "@axe-core/react": "^4.2.2",
        "@babel/core": "^7.15.5",
        "@babel/node": "^7.15.4",
        "@babel/plugin-transform-typescript": "^7.15.4",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-typescript": "^7.15.0",
        "@graphql-codegen/cli": "^2.2.0",
        "@graphql-codegen/fragment-matcher": "^3.1.0",
        "@graphql-codegen/typescript": "^2.2.2",
        "@graphql-codegen/typescript-operations": "^2.1.4",
        "@graphql-codegen/typescript-react-apollo": "^3.1.4",
        "@testing-library/dom": "^8.5.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.0",
        "@types/node": "^16.9.1",
        "@types/react": "^17.0.21",
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "concurrently": "^6.2.1",
        "eslint": "^7.32.0",
        "eslint-config-next": "^11.1.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.25.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "next": "^11.1.2",
        "prettier": "^2.4.0",
        "prisma": "^3.0.2",
        "ts-node": "^10.2.1",
        "typegraphql-prisma": "^0.16.0",
        "typescript": "^4.4.3"
    }
