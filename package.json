{
    "name": "nextjs-ts-auto-graphql",
    "version": "1.0.0",
    "private": false,
    "dependencies": {
        "@apollo/client": "^3.4.16",
        "@emotion/react": "^11.6.0",
        "@emotion/styled": "^11.6.0",
        "@mui/material": "^5.2.0",
        "@mui/styles": "^5.2.0",
        "@prisma/client": "^3.5.0",
        "@types/graphql-fields": "^1.3.4",
        "apollo-server-core": "^3.5.0",
        "apollo-server-micro": "^3.5.0",
        "class-validator": "^0.13.2",
        "graphql": "^15.6.1",
        "graphql-fields": "^2.0.3",
        "graphql-scalars": "^1.13.6",
        "jest-esm-transformer": "^1.0.0",
        "micro": "^9.3.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.20.2",
        "reflect-metadata": "^0.1.13",
        "ts-jest": "^27.0.7",
        "type-graphql": "^1.1.1"
    },
    "keywords": [
        "Next.js",
        "GraphQL",
        "TypeScript",
        "Prisma",
        "Apollo",
        "ESLint",
        "Prettier",
        "React",
        "React Hook Form",
        "Jest",
        "Testing Library React",
        "Material-UI"
    ],
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "scripts": {
        "build": "next build",
        "dev": "prisma generate && concurrently \"npm run server-dev\" \"npm run watch-queries\" \"npm run type-check:watch\"",
        "format": "prisma format &&  prettier --write . && eslint --fix .",
        "lint": "next lint",
        "start": "next start",
        "test": "jest",
        "type-check": "tsc",
        "type-check:watch": "tsc --watch --preserveWatchOutput",
        "quality": "npm install --silent && npm run type-check && npm run lint && npm run test",
        "generate": "prisma generate",
        "migrate": "prisma migrate dev",
        "deploy": "prisma migrate deploy",
        "reset": "prisma migrate reset",
        "seed": "prisma db seed",
        "server-dev": "next",
        "studio": "prisma studio",
        "prisma": "prisma format && npm run migrate && npm run seed",
        "generate-types": "graphql-codegen --config codegen.yml",
        "watch-queries": "graphql-codegen --config codegen.yml --watch",
        "postinstall": "prisma generate",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@axe-core/react": "^4.3.1",
        "@babel/core": "^7.16.0",
        "@babel/node": "^7.16.0",
        "@babel/plugin-transform-react-jsx": "^7.16.0",
        "@babel/plugin-transform-typescript": "^7.16.1",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-typescript": "^7.16.0",
        "@graphql-codegen/cli": "^2.3.0",
        "@graphql-codegen/fragment-matcher": "^3.2.0",
        "@graphql-codegen/typescript": "^2.4.1",
        "@graphql-codegen/typescript-operations": "^2.2.1",
        "@graphql-codegen/typescript-react-apollo": "^3.2.2",
        "@next/eslint-plugin-next": "^12.0.4",
        "@testing-library/dom": "^8.11.1",
        "@testing-library/jest-dom": "^5.15.1",
        "@testing-library/react": "^12.1.2",
        "@types/node": "^16.11.10",
        "@types/react": "^17.0.36",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.3.1",
        "concurrently": "^6.4.0",
        "eslint": "^7.32.0",
        "eslint-config-next": "^12.0.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "husky": "^7.0.4",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.3.1",
        "next": "^12.0.4",
        "prettier": "^2.4.1",
        "prisma": "^3.5.0",
        "ts-node": "^10.4.0",
        "typegraphql-prisma": "^0.17.0",
        "typescript": "^4.5.2"
    }
}
